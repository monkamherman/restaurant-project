{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "node/no-unsupported-features/es-syntax": "off",
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  }