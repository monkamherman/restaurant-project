  "license": "MIT",
  "main": "index.html",
  "keywords": [
    "list",
    "of",
    "keywords"
  ],
  "type": "module",
  "engines": {
    "node": ">=v20.18.1",
    "yarn": ">=1.22.22",
    "npm": ">=10.8.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vite build",
    "build:test": "NODE_ENV=production rimraf /tmp/dist && vite build --outDir /tmp/dist",
    "docker:build": "docker build -t react-started .",
    "docker:start": "docker run -p 4001:3000 react-started",
    "clean": "rimraf dist",
    "start": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "vitest --run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:preview": "serve coverage -l 4003",
    "test:clean": "rimraf coverage",
    "ci": "yarn lint && yarn test && yarn build",
    "docs": "typedoc",
    "docs:preview": "serve docs -l 4002",
    "docs:clean": "rimraf docs",
    "reinstall": "yarn clean && rimraf node_modules && yarn cache clean && yarn install",
    "commitlint": "commitlint",
    "prepare": "husky install",
    "prepare:pre-commit": "npx husky add .husky/pre-commit \"yarn lint\"",
    "release": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "lint-staged": {
    "*.{ts, js, tsx, jsx}": [
      "eslint --fix --max-warnings=10 --cache --config eslint.config.js",
      "prettier --write --config .prettierrc"
    ],
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix",
      "prettier --write"
    ]
  },
